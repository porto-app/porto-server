<h2>Add a New Profile</h2>
<form action="/profiles" method="POST">

    <div class="form-group">
        <label for="eMail">Email:</label>
        <input type="text" id="eMail" name="eMail" class="form-control"/ required>
    </div>

    <div class="form-group">
        <label for="firstName">First Name:</label>
        <input type="text" id="firstName" name="firstName" class="form-control" /required>
    </div>

    <div class="form-group">
        <label for="middleName">Middle Name:</label>
        <input type="text" id="middleName" name="middleName" class="form-control" />
    </div>

    <div class="form-group">
        <label for="lastName">Last Name:</label>
        <input type="text" id="lastName" name="lastName" class="form-control" /required>
    </div>

    <div class="form-group">
        <label for="title">Title:</label>
        <input type="text" id="title" name="title" class="form-control" />
    </div>

    <div class="form-group">
        <label for="location">Location:</label>
        <input type="text" id="location" name="location" class="form-control" />
    </div>

    <div class="form-group">
        <label for="urlPic">Profile Pic URL:</label>
        <input type="text" id="urlPic" name="urlPic" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project1Name">Project One Name:</label>
        <input type="text" id="Project1Name" name="Project1Name" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project1Description">Project One Description:</label>
        <input type="text" id="Project1Description" name="Project1Description" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project1URL">URL to Project One:</label>
        <input type="text" id="Project1URL" name="Project1URL" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project1Img">URL to Project One Image:</label>
        <input type="text" id="Project1Img" name="Project1Img" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project2Name">Project Two Name:</label>
        <input type="text" id="Project2Name" name="Project2Name" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project2Description">Project Two Description:</label>
        <input type="text" id="Project2Description" name="Project2Description" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project2URL">URL to Project Two:</label>
        <input type="text" id="Project2URL" name="Project2URL" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project2Img">URL to Project Two Image:</label>
        <input type="text" id="Project2Img" name="Project2Img" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project3Name">Project Three Name:</label>
        <input type="text" id="Project3Name" name="Project3Name" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project3Description">Project Three Description:</label>
        <input type="text" id="Project3Description" name="Project3Description" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project3URL">URL to Project Three:</label>
        <input type="text" id="Project3URL" name="Project3URL" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project3Img">URL to Project Three Image:</label>
        <input type="text" id="Project3Img" name="Project3Img" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project4Name">Project Four Name:</label>
        <input type="text" id="Project4Name" name="Project4Name" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project4Description">Project Four Description:</label>
        <input type="text" id="Project4Description" name="Project4Description" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project4URL">URL to Project Four:</label>
        <input type="text" id="Project4URL" name="Project4URL" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Project4Img">URL to Project Four Image:</label>
        <input type="text" id="Project4Img" name="Project4Img" class="form-control" />
    </div>

    <input type="submit" class="btn btn-primary" />
    <a href="/profiles" class="btn btn-secondary">Cancel</a>
</form>
